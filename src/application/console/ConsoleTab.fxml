<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.console.ConsoleTabController">
   <VBox spacing="20" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <padding>
         <Insets right="20" top="20" bottom="20" left="20"/>
      </padding>

      <HBox spacing="20">
         <TextField fx:id="commandField" prefWidth="400" />
         <Button fx:id="enterButton"  text="Enter" minWidth="50" />
         <Hyperlink fx:id="helpLink" text="?"/>
      </HBox>

      <TextArea fx:id="resultArea"  prefWidth="10000" prefHeight="10000" />
   </VBox>
</AnchorPane>
