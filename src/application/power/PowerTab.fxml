<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.power.PowerTabController">
   <children>
      <Label layoutX="50.0" layoutY="58.0" text="Battery Level (%):" />
      <Slider fx:id="batterySlider" layoutX="49.0" layoutY="84.0" />
      <Label fx:id="batteryLabel" layoutX="170.0" layoutY="58.0" text="0" />

      <Label layoutX="300.0" layoutY="58.0" text="Battery Health" />
      <ComboBox fx:id="batteryHealth" layoutX="300.0" layoutY="79.0" prefWidth="150.0">
          <items>
              <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Dead" />
                  <String fx:value="Failure" />
                  <String fx:value="Good" />
                  <String fx:value="Unknown" />
                  <String fx:value="Overheat" />
                  <String fx:value="Overvoltage" />
              </FXCollections>
          </items>
      </ComboBox>

       <Label layoutX="50.0" layoutY="164.0" text="Battery Status" />
       <ComboBox fx:id="batteryStatus" layoutX="49.0" layoutY="188.0" prefWidth="150.0">
           <items>
               <FXCollections fx:factory="observableArrayList">
                   <String fx:value="Unknown" />
                   <String fx:value="Not charging" />
                   <String fx:value="Discharging" />
                   <String fx:value="Charging" />
                   <String fx:value="Full" />
               </FXCollections>
           </items>
       </ComboBox>

      <Label layoutX="300.0" layoutY="164.0" text="Charging" />
      <ComboBox fx:id="charging" layoutX="300.0" layoutY="188.0" prefWidth="150.0">
          <items>
              <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Charging" />
                  <String fx:value="Not charging" />
              </FXCollections>
          </items>
      </ComboBox>
   </children>
</AnchorPane>
