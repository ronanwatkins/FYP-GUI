<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.utilities.ADBConnectionController">
   <children>
      <Label layoutX="25.0" layoutY="27.0" text="Select device to connect to" />
      <ListView fx:id="devicesListView" layoutX="21.0" layoutY="55.0" prefHeight="79.0" prefWidth="153.0" />
      <Button fx:id="connectButton" onAction="#handleConnectButtonClicked" layoutX="20.0" layoutY="169.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="Connect" />
      <Button fx:id="connectWifiButton" onAction="#handleConnectWifiClicked" layoutX="21.0" layoutY="213.0" mnemonicParsing="false" text="Connect over WiFi" />
   </children>
</AnchorPane>
